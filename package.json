{
  "name": "svelte-icons",
  "version": "0.0.0",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/.gitkeep"
  ],
  "scripts": {
    "build": "bun run clean:icons && bun run build:icons && svelte-package -o dist -i src",
    "build:icons": "bun run scripts/buildIcons.ts node_modules/@canonical/ds-assets/icons/ src/icons",
    "clean:icons": "rm -f src/icons/*.svelte src/icons/index.ts",
    "check": "bun run check:biome && bun run check:ts",
    "check:fix": "bun run check:biome:fix && bun run check:ts",
    "check:biome": "biome check",
    "check:biome:fix": "biome check --write",
    "check:ts": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@canonical/biome-config": "^0.10.0-experimental.4",
    "@canonical/ds-assets": "^0.10.0-experimental.6",
    "@sveltejs/package": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.6",
    "@types/bun": "^1.3.3",
    "@types/node": "^24.10.1",
    "svelte-check": "^4.3.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  }
}
